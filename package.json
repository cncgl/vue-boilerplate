{
  "name": "vue-boilerplate",
  "description": "A Webpack boilerplate with vue-loader,vue-resource and vue-router",
  "author": "re-fort",
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --port 3000",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --progress --hide-modules",
    "clean": "rimraf ./dist"
  },
  "dependencies": {
    "babel-runtime": "6.9.2",
    "bootstrap": "^3.3.7",
    "bulma": "0.1.0",
    "font-awesome": "4.6.3",
    "vue": "1.0.26",
    "vue-resource": "0.9.3",
    "vue-router": "0.7.13"
  },
  "devDependencies": {
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-preset-es2015": "6.9.0",
    "cross-env": "1.0.8",
    "css-loader": "0.23.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "jade": "1.11.0",
    "json-loader": "0.5.4",
    "node-sass": "3.8.0",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.0",
    "style-loader": "0.13.1",
    "vue-hot-reload-api": "1.3.2",
    "vue-html-loader": "1.2.3",
    "vue-loader": "8.5.3",
    "vue-style-loader": "1.0.0",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  }
}
